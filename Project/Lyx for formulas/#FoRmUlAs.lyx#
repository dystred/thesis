\begin_layout Section*
Formulas used in thesis - Marie Dyveke Olafsen
\end_layout

\begin_layout Standard
Implied volatility spread:
\begin_inset Formula 
\[
IV_{call}-IV_{put}=IV_{spread}
\]

\end_inset


\begin_inset Formula 
\[
VS_{i,t}=\sum_{j=1}^{N_{i,t}}w_{j,t}^{i}\left(IV_{j,t}^{i,call}-IV_{j,t}^{i,put}\right)
\]

\end_inset


\begin_inset Formula 
\[
0\leq IV_{j,t}^{i,call}≤1.5\;0≤IV_{j,t}^{i,put}≤1.5
\]

\end_inset


\begin_inset Formula 
\[
\mathcal{B}_{k,t}=\text{Percentile}_{\rho_{k}}\left(S_{i,t}\right)
\]

\end_inset


\begin_inset Formula 
\[
P_{k,t}=\left\{ i|\mathcal{B}_{k-1,t}≤S_{i,t}<\mathcal{B}_{k,t}\right\} 
\]

\end_inset


\begin_inset Formula 
\[
r_{k,t}=\frac{\sum_{i=1}^{N_{k,t}}MV_{i,t-1}\cdot r_{i,t}}{\sum_{i=1}^{N_{k,t}}MV_{i,t-1}}
\]

\end_inset


\begin_inset Formula 
\[
\mathcal{B}_{k,j,t}^{2}=\text{Percentile}_{\rho_{j}}\left(S_{i,t}^{2}|\mathcal{B}_{k,t}^{1}\leq S_{i,t}^{1}<\mathcal{B}_{k,t}^{1}\right)
\]

\end_inset


\begin_inset Formula 
\[
P_{k,j,t}=\left\{ i|\mathcal{B}_{k-1,j,t}^{1}≤S_{i,t}<\mathcal{B}_{k,j,t}^{1}\right\} \cap\left\{ i|\mathcal{B}_{k,j-1,t}^{2}≤S_{i,t}<\mathcal{B}_{k,j,t}^{2}\right\} 
\]

\end_inset


\begin_inset Formula 
\[
\begin{array}{c}
H_{0}:\Delta\leq0\\
H_{1}:\Delta>0
\end{array}
\]

\end_inset


\end_layout

